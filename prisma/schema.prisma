generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Blog {
  id         Int      @id @default(autoincrement())
  title      String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @default(now())
  content    String
  categoryId Int
  category   Category @relation(fields: [categoryId], references: [id])
}

model Category {
  id    Int    @id @default(autoincrement())
  title String
  blogs Blog[]
}

model Project {
  id    Int    @id @default(autoincrement())
  title String
}
